generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  username String
  password String
}

model DirectLink {
  id           String @id
  viewCount    Int    @default(0)
  userId       String
  originalLink String
  shortenLink  String
}
